{
  "multipart": [
    {
      "when": {
        "connection_down": "disconnected",
        "connection_up": "disconnected",
        "connection_north": "disconnected",
        "connection_south": "disconnected",
        "connection_west": "disconnected",
        "connection_east": "disconnected"
      },
      "apply": {
        "model": "goo:block/goo_pipe",
        "uvlock": true
      }
    },
    {
      "when": {
        "connection_down": "connected",
        "connection_up": "connected"
      },
      "apply": {
        "model": "goo:block/goo_pipe_through_connection",
        "x": 180,
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "connection_north": "connected",
        "connection_south": "connected"
      },
      "apply": {
        "model": "goo:block/goo_pipe_through_connection",
        "x": 270,
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "connection_west": "connected",
        "connection_east": "connected"
      },
      "apply": {
        "model": "goo:block/goo_pipe_through_connection",
        "x": 90,
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "connection_down": "connected",
        "connection_up": "disconnected"
